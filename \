#include <stdlib.h>
#include <stdio.h>
#include "malloc.h"
#include <sys/mman.h>

void	*new_ptr(int size, void *ptr)
{
	if (ptr == NULL)
		;//init struct (size);
	else
	{
		while (ptr->next && ptr->dispo < size)
			ptr = ptr->next;
		if (size > ptr->dispo)
			//ptr->next = init struc (size)
		else
		{
			
		}
	}
	return ((void *)ptr);
}


void	*malloc(size_t size)
{
	static t_mal *small;
	static t_mal *big;
	static t_mal *other;
	int			n;
	int			m;

	if (size < n)
		new_ptr(size, small);
	else if (size < m)
		new_ptr(size, big);
	else
		new_ptr(size, other);
	return (NULL);
}
